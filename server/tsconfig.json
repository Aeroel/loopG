{ 
  "compilerOptions": {
    "outDir": "./built",
    "strict": true,
    "moduleResolution": "nodenext",
    "verbatimModuleSyntax": true,
    "baseUrl": ".",
    "module": "nodenext",
    "target": "esnext",
    "lib": [
      "ESNext"
    ],
  "paths": {
    "#root/*": ["./built/*"],
  }
  },
  "exclude": [
    "node_modules",
    "**/node_modules/*",
    "./built"
  ],
  "include": ["./"]
}